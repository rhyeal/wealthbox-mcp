  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/wealthbox-mcp.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
