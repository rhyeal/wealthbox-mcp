{
  "name": "wealthbox-mcp",
  "version": "0.1.2",
  "type": "module",
  "description": "Model Context Protocol server for Wealthbox API",
  "main": "dist/index.js",
  "bin": {
    "wealthbox-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node --transpile-only src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
